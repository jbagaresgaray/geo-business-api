{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-immutable"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "no-console": false,
    "variable-name": false,
    "object-literal-sort-keys": {
      "options": "match-declaration-order"
    },
    "prefer-template": true,
    "no-empty": false
  },
  "rulesDirectory": []
}
